<%-include("./include/head.ejs", {
  title: "OnBeat - Internal Server Error",
  description: "Oops, occurred an error!"
})%>
<%-include("./include/nav.ejs")%>
<div class="flex flex-col items-center justify-center w-full h-screen lg:h-screen transition-all" id="500">
  <p class="text-2xl">Internal server error</p>
  <p>If the error persists, please report it to us.</p>
  <br>
  <a href='https://github.com/OnBeat-Project/OnBeat-Bot/issues/new' class="text-center flex items-center justify-center gap-x-2 bg-slate-900 text-white dark:bg-slate-100 dark:text-black p-2 rounded-xl mb-3"><i data-feather="github"></i>Report via Github</a>
  <a href="https://discord.gg/VybAzGs4wB" class="text-center flex items-center justify-center gap-x-2  bg-blue-900 text-white dark:bg-blue-100 dark:text-black p-2 rounded-xl mb-3"><i class="fa-brands fa-discord"></i>Report via Support server</a>
  <br>
  <br>
  <button class="" onclick="expand()" id="expandbtn">Expand error</button>
  <p id="error" class="hidden text-red-600"><%= error.message %></p>
</div>
<script>
  function expand() {
    document.getElementById("error").classList.remove("hidden");
    document.getElementById("expandbtn").classList.add("hidden")
  }
</script>
<%-include('./include/footer.ejs')%>